<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ale.proyectofinal" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".login"
            android:label="@string/title_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".votacion"
            android:label="@string/title_activity_votacion" >
        </activity>
        <activity
            android:name=".confirmacion"
            android:label="@string/title_activity_confirmacion" >
        </activity>
        <activity
            android:name=".VotacionCompleta"
            android:label="@string/title_activity_votacion_completa" >
        </activity>
    </application>

</manifest>
